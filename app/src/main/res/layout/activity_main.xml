<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.classichu.classicitemselector.MainActivity">

    <TextView
        android:id="@+id/id_gotoselet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        android:padding="30dp"
       />

    <TextView
        android:id="@+id/id_gotoselet_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World! 2"
        android:drawableRight="@drawable/ic_close_black_24dp"
        android:padding="30dp"
        />


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:tint="@color/colorAccent"
        />
    <Button
        android:id="@+id/id_gotoselet_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World! 3"
        android:padding="30dp"
        />

</LinearLayout>
